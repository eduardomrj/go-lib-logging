{
  "name": "eduardomrj/go-lib-logging",
  "description": "Biblioteca de logging para PHP 8.2/Adianti 7.5 baseada em Monolog",
  "type": "library",
  "license": "proprietary",
  "keywords": ["adianti", "logging", "monolog", "whoops"],
  "require": {
    "php": "^8.2",
    "filp/whoops": "^2.18.4",
    "monolog/monolog": "^3.9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Lib\\GOlib\\Log\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {"sort-packages": true},
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}