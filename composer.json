{
  "name": "eduardomrj/go-lib-logging",
  "description": "Biblioteca de logging para PHP 8.2/Adianti 7.5 baseada em Monolog",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/eduardomrj/go-lib-logging",
  "keywords": [
    "adianti",
    "logging",
    "monolog",
    "whoops"
  ],
  "authors": [
    {
      "name": "Eduardo Martins",
      "email": "eduardo@sysautomacao.com.br"
    }
  ],
  "require": {
    "php": "^8.2",
    "filp/whoops": "^2.18.4",
    "monolog/monolog": "^3.9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "GOlib\\Log\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "support": {
    "issues": "https://github.com/eduardomrj/go-lib-logging/issues"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}